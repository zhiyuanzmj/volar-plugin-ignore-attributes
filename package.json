{
  "name": "volar-plugin-ignore-attributes",
  "type": "module",
  "version": "0.2.0",
  "packageManager": "pnpm@9.11.0",
  "description": "A volar plugin to ignore unocss attributes for jsx.",
  "author": "zhiyuanzmj <zhiyuanzmj@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/zhiyuanzmj",
  "homepage": "https://github.com/zhiyuanzmj/volar-plugin-ignore-attributes#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhiyuanzmj/volar-plugin-ignore-attributes.git"
  },
  "bugs": "https://github.com/zhiyuanzmj/volar-plugin-ignore-attributes/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "muggle-string": "^0.4.1",
    "ts-macro": "^0.0.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@antfu/ni": "^0.23.0",
    "@antfu/utils": "^0.7.10",
    "@types/node": "^22.9.0",
    "bumpp": "^9.8.1",
    "eslint": "^9.14.0",
    "esno": "^4.8.0",
    "lint-staged": "^15.2.10",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0",
    "vite": "^5.4.10",
    "vitest": "^2.1.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
